<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel styleName='{style.outer}'>
    <div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div>
    <div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div>
    <div class='{style.label}'>Street Address</div><div class='{style.value}' ui:field='streetAddress'></div>
    <div class='{style.label}'>City</div><div class='{style.value}' ui:field='city'></div>
    <div class='{style.label}'>State</div><div class='{style.value}' ui:field='state'></div>
    <div class='{style.label}'>Zip</div><div class='{style.value}' ui:field='zip'></div>
    <div class='{style.label}'>Person</div><div class='{style.value}' ui:field='person'></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
